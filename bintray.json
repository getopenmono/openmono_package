{
	"package": {
		"name": "OpenMono SDK",
		"repo": "OpenMono-Package",
		"subject": "openmono",
		"desc": "Travis CI build of OpenMono SDK package",
		"website_url": "developer.openmono.com",
		"issue_tracker_url": "https://github.com/getopenmono/mono_framework/issues",
		"vcs_url": "https://github.com/getopenmono/mono_framework.git",
		"github_use_tag_release_notes": true,
		"github_release_notes_file": "RELEASE.txt",
		"licenses": ["MIT"],
		"public_download_numbers": false,
		"public_stats": false
	},

	"version": {
		"name": "1.5",
		"desc": "This is a version",
		"released": "2015-01-04",
		"vcs_tag": "0.5",
		"gpgSign": false
	},

	"files":
	[
		{
			"includePattern": "build/bin(.*)*/(.*\\.gem)", 
			"excludePattern": ".*/do-not-deploy/.*", 
			"uploadPattern": "gems/$2"
		},
		{
			"includePattern": "build/docs/(.*)", 
			"uploadPattern": "docs/$1"
		}
	],
	"publish": true
}